vertex_program Examples/BumpMapVP glsl {
    source bump_mapping_vs.glsl
    default_params {
        param_named_auto lightPosition light_position_object_space 0
		param_named_auto worldViewProj worldviewproj_matrix
		param_named_auto lightNumber light_number 0
    }
}

fragment_program Examples/BumpMapFP glsl {
    source bump_mapping_ps.glsl
    default_params {
        param_named_auto lightDiffuse light_diffuse_colour 0
    }
}
