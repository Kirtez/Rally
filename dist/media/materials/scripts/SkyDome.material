material Rally/CloudySky
{
	technique
	{
		pass
		{
			lighting off
			depth_write off

			texture_unit
			{
				texture clouds.jpg gamma

				scroll_anim 0.03 0
			}
		}
	}
	
	// Don't interfere with the G-buffer
	technique {
	    scheme GBufferScheme
	    pass {
	        lighting off
	        depth_write off
	        color_write off
	    }
	}
}
