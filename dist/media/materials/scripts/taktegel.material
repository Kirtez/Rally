material taktegel
{
	technique
	{
		// First base ambient light pass
		pass
		{
			lighting on
			ambient 0.4 0.4 0.4
			diffuse 1.0 1.0 1.0
		}
		
		//Texture pass: Texture multiplied by amount of light from bump map pass
		pass
		{
			lighting off
			scene_blend modulate
			texture_unit
			{
				texture taktegel.png
			}
		}
	}
}

